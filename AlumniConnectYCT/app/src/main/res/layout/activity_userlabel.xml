<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@android:color/white" >
    <include 
        layout="@layout/view_navigation"/>
    <TextView
        android:text="标签个数限3个" 
        android:paddingLeft="30dip"
        android:gravity="center_vertical"
    	android:background="@color/regist_title_bakcground"
		android:layout_width="fill_parent"
		android:layout_height="@dimen/regist_title_height"/>
    <LinearLayout 
        android:id="@+id/userinfolabel_layout"
        android:orientation="vertical"
    	android:layout_width="match_parent"
    	android:layout_height="wrap_content">
        
    </LinearLayout>
	<TextView 
	    android:id="@+id/userinfolabel_commit"
        android:orientation="horizontal"
        android:background="#9bd5fb"
        android:text="添加标签"
        android:gravity="center"
    	android:textSize="@dimen/activity_send_left_text"
        android:textColor="@android:color/white"
        android:layout_height="@dimen/regist_content_height"
		android:layout_width="fill_parent" />
	    
</LinearLayout>
