<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/regist_title_bakcground" >
    <include 
        layout="@layout/view_navigation">
        
    </include>
	<LinearLayout 
        android:id="@+id/setting_update"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@android:color/white"
        android:layout_height="@dimen/ucenter_item_height"
   		android:layout_width="match_parent">
        <LinearLayout 
            android:layout_marginLeft="@dimen/ucenter_item_image_margin_left"
            android:orientation="horizontal"
            android:layout_weight="1"
   			android:layout_width="fill_parent"
   			android:layout_height="wrap_content">	            
	        <TextView 
	            android:text="检测新版本"
	            android:textColor="@android:color/black"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
	            android:textSize="@dimen/ucenter_item_text"/>
	        <TextView 
	            android:id="@+id/ucenter_newmessage"
	            android:text="@string/version"
	            android:textColor="@color/version"
	            android:gravity="right"
	            android:layout_marginLeft="5dip"
	            android:textSize="@dimen/version_text"
    			android:layout_width="match_parent"
    			android:layout_height="wrap_content"
				android:src="@drawable/new_notify"/>
        </LinearLayout>
        <ImageView 
            android:src="@drawable/ic_next"
            android:layout_marginLeft="@dimen/ucenter_item_arror_margin_right"
            android:layout_marginRight="@dimen/ucenter_item_arror_margin_right"
   			android:layout_width="wrap_content"
   			android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout 
        android:id="@+id/setting_suggest"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@android:color/white"
        android:layout_marginTop="@dimen/ucenter_item_sper"
        android:layout_height="@dimen/ucenter_item_height"
   		android:layout_width="match_parent">
        <LinearLayout 
            android:orientation="horizontal"
            android:layout_weight="1"
   			android:layout_width="fill_parent"
   			android:layout_height="wrap_content">	            
	        <TextView 
	            android:text="您期待的功能和意见"
            	android:layout_marginLeft="@dimen/ucenter_item_image_margin_left"
	            android:textColor="@android:color/black"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
	            android:textSize="@dimen/ucenter_item_text"/>
        </LinearLayout>
        <ImageView 
            android:src="@drawable/ic_next"
            android:layout_marginRight="@dimen/ucenter_item_arror_margin_right"
   			android:layout_width="wrap_content"
   			android:layout_height="wrap_content"/>
    </LinearLayout>
    <View 
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/regist_title_bakcground"/>
    <LinearLayout 
        android:id="@+id/setting_share"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@android:color/white"
        android:layout_height="@dimen/ucenter_item_height"
   		android:layout_width="match_parent">
        <LinearLayout 
            android:orientation="horizontal"
            android:layout_weight="1"
   			android:layout_width="fill_parent"
   			android:layout_height="wrap_content">	            
	        <TextView 
	            android:text="分享"
            	android:layout_marginLeft="@dimen/ucenter_item_image_margin_left"
	            android:textColor="@android:color/black"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
	            android:textSize="@dimen/ucenter_item_text"/>
        </LinearLayout>
        <ImageView 
            android:src="@drawable/ic_next"
            android:layout_marginRight="@dimen/ucenter_item_arror_margin_right"
   			android:layout_width="wrap_content"
   			android:layout_height="wrap_content"/>
    </LinearLayout>
    <View 
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/regist_title_bakcground"/>
    <LinearLayout 
        android:id="@+id/setting_protocol"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@android:color/white"
        android:layout_marginTop="@dimen/ucenter_item_sper"
        android:layout_height="@dimen/ucenter_item_height"
   		android:layout_width="match_parent">
        <LinearLayout 
            android:orientation="horizontal"
            android:layout_weight="1"
   			android:layout_width="fill_parent"
   			android:layout_height="wrap_content">	            
	        <TextView 
	            android:text="关于"
            	android:layout_marginLeft="@dimen/ucenter_item_image_margin_left"
	            android:textColor="@android:color/black"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
	            android:textSize="@dimen/ucenter_item_text"/>
        </LinearLayout>
        <ImageView 
            android:src="@drawable/ic_next"
            android:layout_marginRight="@dimen/ucenter_item_arror_margin_right"
   			android:layout_width="wrap_content"
   			android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout 
        android:orientation="vertical"
 		android:layout_width="match_parent"
  		android:layout_height="match_parent"
		android:gravity="center">
        <TextView 
	        android:id="@+id/setting_loginout"
	   		android:layout_marginLeft="@dimen/login_edit_margin_leftright"
	      	android:layout_marginRight="@dimen/login_edit_margin_leftright"
	        android:layout_marginTop="@dimen/ucenter_item_sper"
	        android:layout_marginBottom="@dimen/ucenter_item_sper"
	        android:text="注销"
	        android:gravity="center"
	        android:textSize="@dimen/login_edit_commit_text"
	        android:textColor="@android:color/white"
	        android:background="@drawable/bg_btn_darkred"
	   		android:layout_width="match_parent"
	   		android:layout_height="@dimen/activity_send_text2_height"
	        android:textStyle="bold" />
    </LinearLayout>
    
</LinearLayout>
